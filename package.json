{
  "name": "qiankun-vite-test",
  "version": "0.0.2",
  "description": "qiankun demo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": " npm run start",
    "start": "npm-run-all --parallel start:*",
    "start:main": "cd packages/main && npm start",
    "start:child1": "cd packages/vue3Vite && npm start",
    "start:vue2": "cd packages/vue2 && npm start",
    "build": "npm-run-all build:* && bash ./scripts/bundle.sh",
    "build:vue": "cd packages/vue && npm run build",
    "build:main": "cd packages/main && npm run build"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "lerna": "^6.6.2",
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "vite2",
    "vue3",
    "micro",
    "qiankun-example",
    "demo"
  ],
  "author": "mj",
  "license": "MIT"
}
